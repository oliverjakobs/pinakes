{% extends "_base.html.twig" %}

{% block title %}
  Pinakes - {{ entity }}
{% endblock %}

{% block content %}
<div class="detail">
  <h1>{{ entity }}</h1>

  <ul>
  {% for field in fields %}
    <li>{{ field.caption }}: {{ get_value(field, entity)|raw }}</li>
  {% endfor %}
  </ul>

</div>
{% endblock %}
